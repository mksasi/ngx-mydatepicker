{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1, 
  "newProjectRoot": "projects",
  "projects": {
    "ngx-mydatepicker": {
      "projectType": "library",
      "root": "projects/ngx-mydatepicker",
      "sourceRoot": "projects/ngx-mydatepicker/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/ngx-mydatepicker/tsconfig.lib.json",
            "project": "projects/ngx-mydatepicker/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-mydatepicker/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-mydatepicker/src/test.ts",
            "tsConfig": "projects/ngx-mydatepicker/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-mydatepicker/karma.conf.js"
          }
        }
      }
    },
    "ngx-mydatepicker-example": {
      "root": "",
      "sourceRoot": "examples",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/sampleapp",
            "index": "examples/index.html",
            "main": "examples/main.ts",
            "polyfills": "examples/polyfills.ts",
            "tsConfig": "examples/tsconfig.app.json",
            "assets": [
              "examples/favicon.ico",
              "examples/assets"
            ],
            "styles": [
              
            ],
            "scripts": [],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "examples/environments/environment.ts",
                  "with": "examples/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ngx-mydatepicker-example:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ngx-mydatepicker-example:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ngx-mydatepicker-example:build"
          }
        }
      }
    }
  },
  "defaultProject": "ngx-mydatepicker-example"
}
